{
    "name": "gbenm/search_server",
    "type": "project",
    "require": {
        "phpfastcache/phpfastcache": "^9.1",
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^6.4",
        "vlucas/phpdotenv": "^5.5",
        "predis/predis": "^2.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Benyamin Galeano",
            "email": "benyamin.galeano@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy": "^1.16",
        "fakerphp/faker": "^1.21",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "lint": [
            "phpcs --standard=PSR12 src tests bootstrap"
        ],
        "format": [
            "phpcbf --standard=PSR12 src tests bootstrap"
        ]
    }
}
